    "eslint": "^4.18.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "is-typedarray": "^1.0.0",
    "moment": "^2.19.3",
    "moment-timezone": "^0.5.13",
    "momentjs": "^2.0.0",
    "mongoose": "^4.13.11",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.1",
    "passport": "^0.3.2",
