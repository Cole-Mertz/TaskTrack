{
  "name": "TaskTracker",
  "version": "0.0.1",
  "description": "Task Chain Tracking Application",
  "private": true,
  "author": "Kyle Becker, Zach Miller, Cole Waltz",
  "main": "bin/www",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmbecker/TaskTracker.git"
  },
  "scripts": {
    "start": "node bin/www",
    "build": "run-s webpack:prod node:prod",
